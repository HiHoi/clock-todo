{
    "manifest_version": 2,
    "name": "My clock & Todo",
    "description": "Simple clock and Todo list with daliy weather",
    "version" : "2.0",
    "permissions": [
        "geolocation",
        "tabs"
        ],
    "chrome_url_overrides":{
        "newtab" : "main.html"
    },
    "browser_action":{
        "default_icon" : "128.png"
    },
    "background":{
        "scripts": ["background.js"]
    },
    "content_security_policy": "script-src 'self' http://code.jquery.com/jquery-latest.min.js; object-src 'self'"
   
}